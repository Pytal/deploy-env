{
  "name": "deploy-env",
  "description": "Deploy environment variables from .env and .env-cmdrc.json files to Vercel",
  "version": "2.0.1",
  "main": "lib/mod.js",
  "bin": {
    "dpl-env": "lib/cli.js",
    "deploy-env": "lib/cli.js"
  },
  "scripts": {
    "test": "jest --silent",
    "prepublishOnly": "pnpm run build",
    "build": "rm -rf lib && tsc",
    "postbuild": "pnpm run shebang && pnpm run terse",
    "shebang": "node scripts/shebang.mjs lib/cli.js",
    "terse": "sucrase-node scripts/terse.ts lib/"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "jest": "^26.6.3",
    "sucrase": "^3.17.1",
    "terser": "^5.6.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "vercel": "^21.0.0"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=v10.24.0"
  },
  "repository": "github:Pytal/deploy-env",
  "license": "MIT"
}
