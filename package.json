{
  "name": "deploy-env",
  "version": "1.1.1",
  "main": "lib/mod.js",
  "bin": {
    "dpl-env": "lib/cli.js",
    "deploy-env": "lib/cli.js"
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "rm -rf lib && tsc",
    "postbuild": "pnpm run shebang && pnpm run terse",
    "shebang": "node --experimental-top-level-await scripts/shebang.mjs lib/cli.js",
    "terse": "ts-node scripts/terse.ts lib/"
  },
  "dependencies": {
    "env-cmd": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.19",
    "terser": "^4.8.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "vercel": "^19.0.0"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": "github:Pytal/deploy-env"
}
