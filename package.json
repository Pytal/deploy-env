{
  "name": "deploy-env",
  "description": "Deploy environment variables from .env and .env-cmdrc.json files to Vercel.",
  "version": "2.0.1",
  "type": "commonjs",
  "main": "./lib/mod.js",
  "exports": "./lib/mod.js",
  "bin": {
    "dpl-env": "./lib/cli.js",
    "deploy-env": "./lib/cli.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc && pnpm shebang && pnpm terse",
    "prepublishOnly": "pnpm build",
    "shebang": "node scripts/shebang.mjs lib/cli.js",
    "terse": "sucrase-node scripts/terse.ts lib/",
    "test": "jest --silent"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "esbuild": "^0.12.18",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "sucrase": "^3.20.0",
    "terser": "^5.7.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "vercel": "^21.0.0"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=v12.22.4"
  },
  "repository": "github:Pytal/deploy-env",
  "license": "MIT"
}
